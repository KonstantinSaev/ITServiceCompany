    <article  id="main-article">
        <h3 id="main-font">За Нас</h3>
        <div class="row" id="media">
            <div class="col-6">
                <div class="biggest-left-image"></div>
            </div>
            <div class="col-6">
                <div class="row" id="four-small-sections">
